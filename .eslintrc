{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "jasmine": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-underscore-dangle": [ "error", { "allow": [ "_id" ] } ],
        "no-console": [ "warn", { "allow": ["tag"] } ]
    }
}